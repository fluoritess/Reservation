            <arg column="stock" jdbcType="INTEGER" javaType="java.lang.Integer" />
            <arg column="cumulative_evaluation" jdbcType="INTEGER" javaType="java.lang.Integer" />
            <arg column="score" jdbcType="DOUBLE" javaType="java.lang.Double" />
